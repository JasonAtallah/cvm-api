{
  "$id": "http://cannabisvendormgmt.com/schemas/vendor-edibles.json",
  "type": "object",
  "properties": {
    "products": {
      "type": "array",
      "items": {
        "properties": {
          "name": {
            "type": "string",
            "minLength": 1
          },
          "unitsAvailable": {
            "type": ["null", "number"]
          },
          "thc": {
            "type": ["null", "number"]
          },
          "cbd": {
            "type": ["null", "number"]
          },
          "shelfReady": {
            "type": ["null", "boolean"]
          },
          "photos": {
            "type": "array",
            "items": {
              "$ref": "file-object.json"
            }
          },
          "testResults": {
            "type": "array",
            "items": {
              "$ref": "file-object.json"
            }
          }
        }
      }
    }
  }
}
