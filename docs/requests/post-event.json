{
  "method": "POST",
  "header": [
    {
      "key": "Content-Type",
      "value": "application/json"
    },
    {
      "key": "Authorization",
      "value": "Bearer {{BUYER_TOKEN}}"
    }
  ],
  "body": {
    "mode": "raw",
    "raw": "{\n\t\"name\": \"{{event.name}}\",\n\t\"location\": \"{{event.location}}\",\n\t\"date\": \"{{event.date}}\",\n\t\"time\": \"{{event.time}}\",\n\t\"duration\": \"{{event.duration},\n\t\"timezone\": \"{{event.timezone}}\"\n}"
  },
  "url": {
    "raw": "{{API_HOST}}/buyer/v1/events",
    "host": [
      "{{API_HOST}}"
    ],
    "path": [
      "buyer",
      "v1",
      "events"
    ]
  }
}
