{
  "method": "PUT",
  "header": [
    {
      "key": "Content-Type",
      "value": "application/json"
    },
    {
      "key": "Authorization",
      "value": "Bearer {{BUYER_TOKEN}}"
    },
    {
      "key": "origin",
      "value": "{{BUYER_HOST}}"
    }
  ],
  "body": {
    "name": "{{calendar.name}}",
    "timezone": "{{calendar.timezone}}"
  },
  "url": {
    "raw": "{{API_HOST}}/buyer/v1/gcalendar",
    "host": [
      "{{API_HOST}}"
    ],
    "path": [
      "buyer",
      "v1",
      "gcalendar"
    ]
  }
}